import "raylib"

fn init() {
    raylib.InitWindow(800, 450, "raylib-unka [core] example - basic window")
}

fn update() {
    raylib.BeginDrawing()
    raylib.ClearBackground(raylib.RAYWHITE)
    raylib.DrawText("Congrats! You created your first raylib-umka window!", 150, 200, 20, raylib.LIGHTGRAY)
    raylib.EndDrawing()
}

fn close() {
    raylib.CloseWindow()
}

fn main(){}
